<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mvvmpattern">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".base.MvvmApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".module.mvvmmodule.MvvmActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".module.mvpmodule.MvpActivity">
        </activity>
        <activity android:name=".module.mvvmmodule.favouritemodule.FavouriteActivity">
        </activity>
        <activity android:name=".module.mvvmmodule.fragmentmodule.GeFavFragmentActivity">
        </activity>
    </application>

</manifest>